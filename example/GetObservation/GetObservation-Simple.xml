     http://schemas.opengis.net/sos/1.0.0/sosGetObservation.xsd"
    service="SOS" version="1.0.0">

    <offering>ABC</offering>

    <observedProperty>urn:ogc:def:phenomenon:OGC:1.0.30:position</observedProperty>
